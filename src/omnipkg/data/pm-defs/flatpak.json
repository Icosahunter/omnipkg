{
    "name"      :   "flatpak",
    "search"    : [ "flatpak search --columns=name,application,description,remotes {package}", "{name}\t{id}\t{summary}\t{remote}\n" ],
    "install"   :   "flatpak install -y {package}",
    "uninstall" :   "flatpak remove -y {package}",
    "update"    :   "flatpak update {package}",
    "update-all":   "flatpak update",
    "installed" : [ "flatpak list --columns=name,application,description,remotes", "{name}\t{id}\t{summary}\t{remote}\n" ],
    "updatable" : [ "flatpak remote-ls --updates --columns=name,application,description,remotes", "{name}\t{id}\t{summary}\t{remote}\n" ],
    "info"      : [ "flatpak search --columns=name,application,description,remotes {package}", "{name}\t{id}\t{summary}\t{remote}\n" ],
    "remotes"   : [ "flatpak remotes --columns=name,url", "{remote}\t{remote_url}\n"]
}